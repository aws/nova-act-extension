{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["react", "^@core/(.*)$", "<THIRD_PARTY_MODULES>", "^~", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always"
}
